generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Invitado {
  id         String  @id @default(uuid())
  nombre     String
  token      String
  cantidad   Int     @default(2)
  confirmado Boolean @default(false)
  mensaje    String?
}
